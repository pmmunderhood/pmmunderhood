    },
    {
      "created_at": "Tue Nov 10 14:14:48 +0000 2020",
      "id": 1326166422356189200,
      "id_str": "1326166422356189185",
      "full_text": "Hubspot‚Äôs website has many examples of pages with a good feature/benefit balance, e.g. https://t.co/3O7tM8rOP0. Any other examples that you like?",
      "truncated": false,
      "display_text_range": [
        0,
        145
      ],
