    },
    {
      "created_at": "Tue Nov 10 15:13:20 +0000 2020",
      "id": 1326181151669620700,
      "id_str": "1326181151669620742",
      "full_text": "5. Add visuals.\n\nHelp people understand what you're talking about by adding pictures or gifs to your copy. Also, visuals will break up the copy, making it easier to read.",
      "truncated": false,
      "display_text_range": [
        0,
        170
      ],
      "entities": {
        "hashtags": [],
        "symbols": [],
        "user_mentions": [],
        "urls": []
      },
